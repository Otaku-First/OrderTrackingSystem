				{
					"name": "login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Save access_token to collection variable\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    var accessToken = jsonData.access_token;\r",
									"    if (accessToken) {\r",
									"        pm.collectionVariables.set(\"auth_token\", accessToken);\r",
									"    } else {\r",
									"        console.error(\"Access token not found in response.\");\r",
									"    }\r",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
